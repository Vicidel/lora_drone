journal = {Evolutionary computation},
keywords = {Algorithms,Biological Evolution,Computer Simulation,Models, Genetic,Models, Statistical,Mutation,Stochastic Processes},
month = {jan},
number = {2},
pages = {159--95},
pmid = {11382355},
title = {{Completely derandomized self-adaptation in evolution strategies.}},
